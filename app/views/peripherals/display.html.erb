
	<div class="pd-container">		
		<div id= "pd" data-url="<%= :peripherals %>" >
			<% @lasttwo.each do |post| %>
			<div class = "row">	
				<div class="span4 offset4 pd-element">
					
					<h2><%= post.title%></h2>
					<small>updated: <%= post.updated_at %></small>
					<small> - by : <%= post.modified_by %></small>
					
					<div class="progress">
						<div class="bar" style="width: <%= post.progress%>%"></div>
					</div>
				</div>
			</div>
			<% end %>
		</div>
	</div>
	
